            <CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:id="@+id/checkBox24" android:layout_column="4"/>
            <CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:id="@+id/checkBox25" android:layout_column="5"/>
            <CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:id="@+id/checkBox26" android:layout_column="6"/>
            <CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:id="@+id/checkBox27" android:layout_column="7"/>
            <CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content"
