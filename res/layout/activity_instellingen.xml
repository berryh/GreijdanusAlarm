                      android:id="@+id/checkBox33" android:layout_column="6"/>
            <CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:id="@+id/checkBox34" android:layout_column="7"/>
            <CheckBox android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:id="@+id/checkBox35" android:layout_column="8"/>
        </TableRow>
    </TableLayout>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/instellingenscherm_uitleg_werking"
            android:id="@+id/textView14" android:layout_below="@+id/tableLayout"
            android:layout_centerHorizontal="true" android:layout_marginTop="15dp"
