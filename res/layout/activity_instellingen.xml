            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="1"
            android:id="@+id/textView"
            android:layout_alignParentEnd="false"
            android:layout_alignParentStart="false"
            android:layout_marginLeft="10dp"
            android:layout_alignLeft="@+id/tableLayout" android:layout_below="@+id/textView8"
            android:layout_marginRight="12.5dp"/>
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
              android:textAppearance="?android:attr/textAppearanceSmall" android:text="2"
              android:id="@+id/textView2"
              android:layout_alignParentEnd="false"
              android:layout_alignParentStart="false"
              android:layout_alignTop="@+id/textView"
              android:layout_marginLeft="12dp"
              android:layout_toRightOf="@+id/textView" android:layout_marginRight="12.5dp"/>
